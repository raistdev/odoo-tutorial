<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="inherited_res_users_view_form" model="ir.ui.view">
        <field name="name">inherited.res.users.view.form</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook//page[@name='preferences']" position="after">
                <page name="properties" string="Properties">
                    <field name="property_ids" />
                </page>
            </xpath>
        </field>
    </record>
</odoo>
